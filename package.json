{
  "name": "html-editor",
  "version": "0.1.0",
  "private": true,
  "repositiry": "https://github.com/kolserdav/html-editor.git",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "compile": "npm run clean:dist && webpack --env NODE_ENV=production",
    "compile:web": "npm run clean:umd && webpack --env NODE_ENV=production MIN=false -c webpack.web.config.js",
    "compile:web-min": "webpack --env NODE_ENV=production MIN=true -c webpack.web.config.js",
    "webpack": "webpack",
    "build": "cross-env CI=false react-scripts build",
    "clean:dist": "rimraf dist",
    "clean:umd": "rimraf umd"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.12",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.51.0",
    "airbnb": "^0.0.2",
    "dotenv-webpack": "^8.0.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mini-css-extract-plugin": "^2.7.2",
    "prettier": "^2.8.4",
    "sass": "^1.58.0",
    "ts-loader": "^9.4.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "fhead": {
    "root": "src",
    "repository": "https://github.com/kolserdav/html-editor.git",
    "patterns": [
      ".js",
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "dist",
      "umd"
    ],
    "name": "Sergey Kolmiller",
    "email": "uyem.ru@gmail.com",
    "license": "MIT",
    "licenseText": "See in LICENSE file",
    "copyright": "kolserdav, All rights reserved (c)",
    "renewAll": false
  }
}
